					".mm",
					".php",
					".rb",
					".asm",
					".s",
					".swift",
					".kt",
					".kts",
					".go",
					".scala",
					".hs",
					".vhd",
					".vhdl",
					".v",
					".vh",
					".sql",
					".json",
					".yaml",
					".yml",
					".xml",
					".ini",
					".toml",
					".dockerignore",
