{
	"$schema": "https://raw.githubusercontent.com/CodeEditorLand/Tauri/Current/tooling/cli/schema.json",
	"productName": "editor",
	"version": "0.0.0",
	"identifier": "com.tauri.dev",
	"build": {
		"beforeDevCommand": "pnpm run Start --filter=@",
		"devUrl": "http://localhost:9999",
		"beforeBuildCommand": "pnpm run prepublishOnly",
		"beforeBundleCommand": "pnpm run prepublishOnly",
		"frontendDist": "../Seed/Target",
		"features": [],
		"runner": ""
	},
	"app": {
		"windows": [
			{
				"title": "Editor",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		},
		"macOSPrivateApi": false,
		"trayIcon": null,
		"withGlobalTauri": false
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
